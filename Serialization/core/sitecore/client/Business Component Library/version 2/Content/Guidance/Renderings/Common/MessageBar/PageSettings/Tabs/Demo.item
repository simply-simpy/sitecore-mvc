    <r id="{6BEE0A52-2765-436D-AF0F-DFE43C2F508E}" par="Id=DemoActionLink&amp;ConfigurationDetails=ConfigurationItem%3d%7b372A63A0-EB83-400B-864C-6B3AE5E0A09D%7d&amp;Description=The+ConfigurationItem+property+specifies+a+MessageBar+configuration+item.+You+specify+messages+as+child+items+(based+on+the+Message+item+template)+under+this+configuration+item.+You+specify+the+links+(%22actions%22)+next+to+message+by+adding+items+based+on+the+Action+item+template+as+child+items+of+the+MessageBar+item.+The+Text+and+Action+fields+specify+the+text+and+the+link.&amp;Title=Specifying+messages+and+actions" ph="Page.Body" uid="{4E236162-E3D1-48D9-8194-72C30373681A}" />
