    <r id="{6BEE0A52-2765-436D-AF0F-DFE43C2F508E}" par="Id=DemoComponentPanel1&amp;ConfigurationDetails=Database%3d%24context_contentdatabase%26SearchQuery%3dpage%26DropList+DynamicData%3d%7bBinding+DataSource1.SearchQuery%7d&amp;Description=This+demo+shows+that+the+component%27s+default+behavior+is+to+load+data+when+the+page+and+the+component+have+finished+loading.+The+component+queries+the+database+specified+in+the+Database+property+for+the+term+specified+in+the+SearchQuery+property.+The+DynamicData+property+of+the+DropList+control+binds+to+the+Items+property+and+the+DropList+control+shows+the+result+of+the+query.&amp;Title=Loading+data+when+page+has+loaded" ph="Border1.Content" uid="{7D63A65B-A040-42F5-BEF4-484F74ABC1B0}" />
